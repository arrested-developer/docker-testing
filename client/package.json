{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": "${COVERAGE_THRESHOLD}",
        "functions": "${COVERAGE_THRESHOLD}",
        "lines": "${COVERAGE_THRESHOLD}",
        "statements": "${COVERAGE_THRESHOLD}"
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/serviceWorker.js",
      "!src/index.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
